{
  "name": "@kofeinstyle/tradernet-sdk",
  "version": "0.1.0-alpha.0",
  "type": "module",
  "description": "Tradernet API javascript client",
  "homepage": "https://github.com/kofeinstyle/tradernet-sdk",
  "author": "Yefimov Maksym",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
